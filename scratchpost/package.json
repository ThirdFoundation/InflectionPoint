{
  "name": "scratchpost",
  "version": "1.0.0",
  "description": "Node/Express with server side rendered React",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=env,stage-1",
    "build": "babel-node src/build.js",
    "coverage": "node_modules/istanbul/lib/cli.js cover _mocha -- --compilers js:babel-core/register --recursive $(find . -iname '*.test.js' -not -path './node_modules/*')",
    "start": "npm run clean-dist && npm run start-nodemon",
    "start-nodemon": "nodemon --exec npm run babel-node src/server/index.js  --ignore src/static/scripts --ignore node_modules",
    "clean-dist": "npm run remove-dist && mkdir ./src/static/dist",
    "remove-dist": "node_modules/.bin/rimraf ./src/static/dist",
    "start:server": "babel-node src/server/index.js"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "dependencies": {
    "@newrelic/native-metrics": "^2.1.2",
    "babel-cli": "^6.10.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^6.2.4",
    "babel-plugin-react-display-name": "^2.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-register": "^6.24.1",
    "body-parser": "^1.18.2",
    "colors": "^1.1.2",
    "compression": "^1.6.2",
    "cookie-parser": "^1.4.3",
    "debug": "^2.6.0",
    "dnscache": "^1.0.0",
    "eslint": "^3.13.0",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "firebase": "^4.8.1",
    "helmet": "^3.3.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdom": "^11.3.0",
    "mocha": "^3.2.0",
    "mocha-jenkins-reporter": "^0.2.3",
    "newrelic": "^2.2.2",
    "nodemon": "^1.14.7",
    "npm-run-all": "^4.1.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^4.4.5",
    "react-router-dom": "^4.2.2",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "request": "^2.79.0",
    "request-promise": "^4.2.2",
    "serve-favicon": "^2.4.5",
    "should": "^13.1.0",
    "transform-loader": "^0.2.4",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.19.1",
    "webworkify-webpack": "^2.0.5",
    "xml2json": "^0.11.0"
  },
  "license": "ISC",
  "devDependencies": {
    "babel-preset-stage-1": "^6.24.1"
  }
}
